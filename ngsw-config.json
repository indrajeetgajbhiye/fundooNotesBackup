{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ],
            "urls": [
                "https://fonts.googleapis.com/css?family=Roboto&display=swap/**",
                "https://fonts.googleapis.com/css?family=Roboto&display=swap/**",
                "https://ssl.gstatic.com/keep/keep.ico/**"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataGroups": [{
        "name": "notes-api",
        "urls": ["http://fundoonotes.incubation.bridgelabz.com/api/notes/getNotesList"],
        "cacheConfig": {
            "strategy": "freshness",
            "maxSize": 30,
            "maxAge": "1h",
            "timeout": "5s"
        }
    }]
}